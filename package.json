{
  "name": "ssb-server",
  "description": "network protocol layer for secure-scuttlebutt",
  "version": "14.1.10",
  "homepage": "https://github.com/ssbc/ssb-server",
  "repository": {
    "type": "git",
    "url": "git://github.com/ssbc/ssb-server.git"
  },
  "bin": {
    "sbot": "./bin.js",
    "ssb-server": "./bin.js"
  },
  "scripts": {
    "start": "node bin start",
    "prepublishOnly": "npm test",
    "test": "rm node_modules/ssb-server; set -e; npm ls && for t in test/*.js; do echo TEST $t; node $t; done && ./test/deps.sh all",
    "reinstall": "rm -rf node_modules package-lock.json; export npm_config_package_lock=true; npm install && npm shrinkwrap",
    "pretest": "./scripts/pretest.js",
    "coverage": "nyc --reporter=lcov npm test"
  },
  "dependencies": {
    "bash-color": "~0.0.3",
    "broadcast-stream": "^0.2.1",
    "cont": "~1.0.3",
    "cross-spawn": "^6.0.5",
    "debug": "^4.1.1",
    "deep-equal": "^1.0.1",
    "explain-error": "^1.0.3",
    "has-network": "0.0.1",
    "ip": "^1.1.5",
    "mdmanifest": "^1.0.8",
    "minimist": "^1.1.3",
    "mkdirp": "~0.5.0",
    "multiblob": "^1.13.0",
    "multiserver": "^3.3.1",
    "multiserver-address": "^1.0.1",
    "muxrpc-validation": "^3",
    "muxrpcli": "^1.0.0",
    "mv": "^2.1.1",
    "osenv": "^0.1.5",
    "pull-cat": "~1.1.5",
    "pull-file": "^1.0.0",
    "pull-many": "~1.0.6",
    "pull-pushable": "^2.2.0",
    "pull-stream": "^3.6.2",
    "rimraf": "^2.4.2",
    "secret-stack": "^6.0.2",
    "ssb-blobs": "^1.1.13",
    "ssb-client": "^4.7.1",
    "ssb-config": "^3.2.5",
    "ssb-db": "^19.1.0",
    "ssb-ebt": "^5.5.3",
    "ssb-friends": "^4.0.0",
    "ssb-gossip": "^1.0.6",
    "ssb-invite": "^2.0.3",
    "ssb-keys": "^7.1.1",
    "ssb-links": "^3.0.2",
    "ssb-ooo": "^1.2.2",
    "ssb-query": "^2.1.0",
    "ssb-ref": "^2.13.9",
    "ssb-replicate": "^1.2.1",
    "ssb-ws": "^5.1.1",
    "stream-to-pull-stream": "^1.6.10",
    "zerr": "^1.0.0"
  },
  "devDependencies": {
    "cat-names": "^2.0.0",
    "dog-names": "~1.0.2",
    "hexpp": "^2.0.0",
    "interleavings": "^1.0.0",
    "npm-install-package": "^2.1.0",
    "nyc": "^13.2.0",
    "pull-abortable": "^4.1.1",
    "pull-bitflipper": "^0.1.0",
    "rng": "^0.2.2",
    "run-series": "^1.1.8",
    "semver": "^5.6.0",
    "ssb-feed": "^2.3.0",
    "ssb-generate": "^1.0.1",
    "ssb-validate": "^4",
    "tape": "^4.9.1",
    "typewiselite": "^1.0.0"
  },
  "nyc": {
    "exclude": [
      "!**/node_modules/",
      "**/node_modules/**/{test,node_modules}/"
    ],
    "include": [
      "*.js",
      "lib/*.js",
      "plugins/*.js",
      "node_modules/{ssb-*,flume*,secret-stack,muxrpc,multiserver}/*.js",
      "node_modules/{ssb-*,flume*,secret-stack,muxrpc,multiserver}/{util,lib,frame,indexes}/*.js"
    ]
  },
  "author": "Paul Frazee <pfrazee@gmail.com>",
  "license": "MIT"
}
