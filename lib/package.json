{
    "name": "ssb-server",
    "description": "network protocol layer for secure-scuttlebutt",
    "version": "16.0.1",
    "homepage": "https://github.com/ssbc/ssb-server",
    "repository": {
        "type": "git",
        "url": "git://github.com/ssbc/ssb-server.git"
    },
    "bin": {
        "sbot": "./bin/sbot",
        "ssb-server": "./bin/ssb-server"
    },
    "main": "lib/index.js",
    "scripts": {
        "compile": "tsc",
        "pretest": "npm run compile",
        "build": "npm run compile",
        "start": "ts-node src/main.ts start",
        "prepublishOnly": "npm test",
        "test": "set -e; for t in test/*.js; do echo TEST $t; node $t; done",
        "test:pretty": "npm run test | tap-spec",
        "reinstall": "rm -rf node_modules package-lock.json npm-shrinkwrap.json; export npm_config_package_lock=true; npm install && npm dedupe && npm shrinkwrap",
        "coverage": "nyc --reporter=lcov npm test"
    },
    "dependencies": {
        "bash-color": "~0.0.3",
        "broadcast-stream": "^0.2.1",
        "cont": "~1.0.3",
        "cross-spawn": "^6.0.5",
        "debug": "^4.1.1",
        "deep-equal": "^1.0.1",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "explain-error": "^1.0.3",
        "has-network": "0.0.1",
        "ip": "^1.1.5",
        "mdmanifest": "^1.0.8",
        "minimist": "^1.1.3",
        "mkdirp": "~0.5.0",
        "multiblob": "^1.13.0",
        "multiserver": "^3.3.1",
        "multiserver-address": "^1.0.1",
        "muxrpc-validation": "^3",
        "muxrpcli": "3",
        "mv": "^2.1.1",
        "osenv": "^0.1.5",
        "prettier": "^2.5.1",
        "pull-cat": "~1.1.5",
        "pull-file": "^1.0.0",
        "pull-many": "~1.0.6",
        "pull-pushable": "^2.2.0",
        "pull-stream": "^3.6.2",
        "rimraf": "^2.7.1",
        "secret-stack": "^6.4.0",
        "ssb-blobs": "1.2.2",
        "ssb-caps": "^1.0.1",
        "ssb-client": "^4.7.9",
        "ssb-config": "^3.2.5",
        "ssb-db": "^20.0.1",
        "ssb-ebt": "^5.6.1",
        "ssb-friends": "^4.1.0",
        "ssb-gossip": "^1.1.0",
        "ssb-invite": "^2.1.2",
        "ssb-keys": "^7.1.1",
        "ssb-links": "^3.0.10",
        "ssb-local": "^1.0.0",
        "ssb-logging": "^1.0.0",
        "ssb-master": "^1.0.2",
        "ssb-no-auth": "^1.0.0",
        "ssb-onion": "^1.0.0",
        "ssb-ooo": "^1.3.3",
        "ssb-plugins": "^1.0.4",
        "ssb-private1": "^1.0.1",
        "ssb-query": "^2.4.0",
        "ssb-ref": "^2.13.9",
        "ssb-replicate": "^1.3.0",
        "ssb-unix-socket": "^1.0.0",
        "ssb-ws": "^6.2.1",
        "stream-to-pull-stream": "^1.6.10",
        "zerr": "^1.0.0"
    },
    "devDependencies": {
        "@types/node": "^17.0.8",
        "@typescript-eslint/eslint-plugin": "^5.9.0",
        "@typescript-eslint/parser": "^5.9.0",
        "cat-names": "^2.0.0",
        "dog-names": "~1.0.2",
        "hexpp": "^2.0.0",
        "interleavings": "^1.0.0",
        "npm-install-package": "^2.1.0",
        "nyc": "^13.2.0",
        "pull-abortable": "^4.1.1",
        "pull-bitflipper": "^0.1.0",
        "pull-paramap": "^1.2.2",
        "rng": "^0.2.2",
        "run-series": "^1.1.8",
        "semver": "^5.6.0",
        "ssb-feed": "^2.3.0",
        "ssb-generate": "^1.0.1",
        "ssb-validate": "^4",
        "tap-spec": "^5.0.0",
        "tape": "^4.9.1",
        "ts-node": "^10.4.0",
        "tsutils": "^3.21.0",
        "typescript": "^4.5.4",
        "typewiselite": "^1.0.0"
    },
    "nyc": {
        "exclude": [
            "!**/node_modules/",
            "**/node_modules/**/{test,node_modules}/"
        ],
        "include": [
            "*.js",
            "lib/*.js",
            "src/*.ts",
            "plugins/*.js",
            "node_modules/{ssb-*,flume*,secret-stack,muxrpc,multiserver}/*.js",
            "node_modules/{ssb-*,flume*,secret-stack,muxrpc,multiserver}/{util,lib,frame,indexes}/*.js"
        ]
    },
    "author": "Paul Frazee <pfrazee@gmail.com>",
